[run]
branch = True
source = skmine
include = */skmine/*
parallel = True
omit =
    **/tests/*
    */setup.py
    */__init__.py

[report]
# see `https://stackoverflow.com/questions/9202723/excluding-abstractproperties-from-coverage-reports`
exclude_lines = 
    pragma : no cover
    pass
    fetch_any
    LXML_INSTALLED
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    except ImportError
    if self.verbose
    if verbose
    _get_tags
    warnings
    return self

show_missing = True
